<%include ./partials/header%>
<div class="campgroundwrap">
<h1 style="text-align: center"> <strong>REGISTRATION PORTAL</strong>        </h1>
<form action="/register" method="POST">
    <div class="row" style="width:30%;margin:0 auto;">
    <div class="form-group">
        <input class="form-control" type="text" name="username" placeholder="enter your name">
    </div>
    <div class="form-group">
        <input class="form-control" type="password" name="password" placeholder="enter password">
    </div>
    <p class="text-center">
    <button class="btn btn-info">SUBMIT</button>
</p>
</div>
</form>
</div>
<%include ./partials/footer%>